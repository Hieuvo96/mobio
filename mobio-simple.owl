            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>Partitioning protein - ATPase</obo:IAO_0000115>
        <rdfs:label>ParA</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MOBIO_0000064 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000064">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MOBIO_0000193"/>
        <obo:IAO_0000115>PifA is encoded on plasmid F and is responsible for the exclusion of phage T7. E. coli cells that carry plasmid F inhibit the growth of bacteriophage T7. </obo:IAO_0000115>
