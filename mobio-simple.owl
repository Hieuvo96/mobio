    <!-- http://purl.obolibrary.org/obo/MOBIO_0000036 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000036">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MOBIO_0000187"/>
