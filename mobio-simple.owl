        <oboInOwl:inSubset rdf:resource="http://purl.obolibrary.org/obo/valid_for_gocam"/>
        <oboInOwl:shorthand>regulates</oboInOwl:shorthand>
        <rdfs:label xml:lang="en">regulates</rdfs:label>
        <rdfs:label>regulates</rdfs:label>
    </owl:ObjectProperty>
    


