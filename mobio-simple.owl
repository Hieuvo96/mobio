            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>Proteins that are involved in the SOS response, which is a global response to DNA damage.</obo:IAO_0000115>
        <rdfs:label>SOS response protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MOBIO_0000195 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000195">
