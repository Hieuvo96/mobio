    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000124">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MOBIO_0000023"/>
        <obo:IAO_0000115>Toxin Antitoxin system Type II</obo:IAO_0000115>
        <rdfs:label>PemI/PemK type II Toxin-antitoxin system</rdfs:label>
    </owl:Class>
    


