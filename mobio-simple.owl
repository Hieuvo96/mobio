        <rdfs:label>Doc/Phd Type II TA System</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MOBIO_0000193 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000193">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MOBIO_0000188"/>
        <obo:IAO_0000115>Genes that allow bacteria to resist infection by bacteriophages have been discovered to be encoded on plasmids.</obo:IAO_0000115>
        <rdfs:label>protein conferring phage resistance</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MOBIO_0000194 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MOBIO_0000194">
